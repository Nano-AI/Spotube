{
  "name": "spotube",
  "version": "0.1.0",
  "private": true,
  "main": "./public/electron.js",
  "description": "Spotify like app where you can import Spotify playlists and play them in in one app, for free!",
  "author": "Nano-AI",
  "build": {
    "appId": "com.nano-ai.spotube"
  },
  "homepage": "./",
  "dependencies": {
    "@craco/craco": "^5.9.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.17",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/usetube": "^2.1.0",
    "@types/uuid": "^8.3.3",
    "discord-rpc": "^4.0.1",
    "electron": "^16.2.2",
    "electron-build": "^0.0.3",
    "electron-is-dev": "^2.0.0",
    "electron-store": "^8.0.1",
    "framer-motion": "^4.1.17",
    "fs": "^0.0.1-security",
    "node-youtube-music": "^0.7.0",
    "path": "^0.12.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-player": "^2.9.0",
    "react-router-dom": "^6.2.1",
    "react-scripts": "^5.0.0",
    "sass": "^1.45.2",
    "typeface-roboto": "^1.1.13",
    "typescript": "^4.5.4",
    "usetube": "^2.1.4",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build": "react-scripts build && electron-build",
    "dev": "concurrently -k \"npm start\" \"yarn run electron\"",
    "electron": "wait-on tcp:3000 && electron ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "concurrently": "^6.5.1",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.7",
    "wait-on": "^6.0.0"
  }
}
